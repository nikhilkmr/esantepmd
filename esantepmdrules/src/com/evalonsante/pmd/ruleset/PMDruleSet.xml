   <rule class="net.sourceforge.pmd.lang.rule.XPathRule" deprecated="false" dfa="false"
         externalInfoUrl=""
         language="java"
         message="An interface name must start with 'I'"
         name="EsanteInterfaceNameRule"
         typeResolution="true">
      <description>Bad example: &#xD;
interface printable{  &#xD;
void print();  &#xD;
}  &#xD;
class A6 implements printable{  &#xD;
public void print(){System.out.println("Hello");}  &#xD;
  &#xD;
public static void main(String args[]){  &#xD;
A6 obj = new A6();  &#xD;
obj.print();  &#xD;
 }  &#xD;
}&#xD;
&#xD;
Good Example:&#xD;
interface Iprintable{  &#xD;
void print();  &#xD;
}  &#xD;
class A6 implements printable{  &#xD;
public void print(){System.out.println("Hello");}  &#xD;
  &#xD;
public static void main(String args[]){  &#xD;
A6 obj = new A6();  &#xD;
obj.print();  &#xD;
 }  &#xD;
}</description>
      <priority>1</priority>
      <properties>
         <property name="xpath">
            <value>//ClassOrInterfaceDeclaration[@Interface='true' and starts-with(@Image, 'I') != @Image]</value>
         </property>
      </properties>
   </rule>
